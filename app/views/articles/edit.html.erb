<!-- app/views/articles/edit.html.erb -->
<h1>Edit Article</h1>

<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>

<%= link_to 'Back', article_path(@article), class: 'btn btn-secondary' %>
